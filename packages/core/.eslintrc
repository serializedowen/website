{
  "extends": "eslint-config-egg/typescript",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "quotes": "off",
    "comma-dangle": "off",
    "array-bracket-spacing":"off",
    "arrow-parens":"off",
    "spaced-comment":"off",
    "@typescript-eslint/ban-ts-comment": "off",
    "@typescript-eslint/indent": "off"
  }
}
